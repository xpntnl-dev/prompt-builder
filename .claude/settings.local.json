{
  "permissions": {
    "allow": [
      "mcp__time__get_current_time",
      "mcp__time__convert_time",
      "mcp__context7__resolve-library-id",
      "mcp__context7__get-library-docs",
      "Bash(mcp__time__get_current_time timezone=\"Europe/London\")"
    ],
    "deny": []
  },
  "mcpServers": {
    "time": {
      "command": "npx",
      "args": [
        "-y",
        "@cloudflare/mcp-server-time"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server"
      ]
    }
  },
  "templateVersion": "6.1.2"
}
